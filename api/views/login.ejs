    <% if(locals.errors) { %>
        <% errors.forEach(error => { %>
            <% if(error === 'InvalidCredentials') { %>
                <p>Incorrect email or password.</p>
            <% } %>
        <% }) %>
    <% } %>
    <form action="/login" method="POST" id="apply">
        <h1>Login</h1>
        <label for="email">Email:</label><br>
        <input type="email" id="email" name="email" value="jeffrey@stjean.ca" required><br>

        <label for="password">Password:</label><br>
        <input type="password" id="password" name="password" value="TestTest123!" required><br>

        <br>
        <input type="submit" value="Log In">
    </form>
    <form action="/user" method="POST" id="create">
        <h1>Create Account</h1>
        <label for="email">Email:</label><br>
        <input type="email" id="email" name="email" value="jeffrey@stjean.ca" required><br>

        <label for="password">Password:</label><br>
        <input type="password" id="password" name="password" value="TestTest123!" required><br>

        <label for="password_verify">Verify Password:</label><br>
        <input type="password" id="password_verify" name="password_verify" value="TestTest123!" required><br>

        <br>
        <input type="submit" value="Submit">
    </form>
</body>
